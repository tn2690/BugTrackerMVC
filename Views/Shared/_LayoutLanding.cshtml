    <script src="~/vendors/bundle.js"></script>

    <!-- Apex chart -->
    <script src="~/vendors/charts/apex/apexcharts.min.js"></script>

    <!-- Daterangepicker -->
    <script src="~/vendors/datepicker/daterangepicker.js"></script>

    <!-- DataTable -->
    <script src="~/vendors/dataTable/datatables.min.js"></script>

    <!-- Dashboard scripts -->
    <script src="~/assets/js/examples/pages/dashboard.js"></script>

    <!-- App scripts -->
    <script src="~/assets/js/app.min.js"></script>

    <!-- Javascript -->
    <script src="~/vendors/ckeditor5/ckeditor.js"></script>

    <!-- ------------------- Gogi scripts end ----------------------------- -->

    <!-- ------------------- Plank scripts start ----------------------------- -->
    <script src="~/plank/library/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/plank/library/jquery-3.6.0.min.js"></script>

    <script src="~/plank/script.js"></script>
    <!-- ------------------- Plank scripts end ----------------------------- -->

